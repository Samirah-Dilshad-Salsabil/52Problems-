#include <bits/stdc++.h>
using namespace std;

int main() {
	string compare="ABCDEFGHIJKLMNOPQRSTUVWXYZ";
	string word;
	cin>> word;
	string new_string="";
	for(int j=0; j<word.length(); j++){
	for(int i=0; i<=compare.length(); i++){
		if(word[j]==compare[i]) {new_string +=to_string(i+1);}}
	} 
	cout<< new_string << endl;
	return 0;
}
